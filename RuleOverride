const domesticNameservers = ["223.5.5.5", "119.29.29.29", "114.114.114.114"];
const foreignNameservers = ["1.1.1.1", "8.8.8.8", "8.8.4.4", "208.67.222.222", "9.9.9.9"];

const dnsConfig = {
  enable: true,
  listen: "127.0.0.1:1053",
  secret: "K!c*ow9!@BgS!6Kw9r",
